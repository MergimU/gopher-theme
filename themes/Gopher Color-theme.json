{
  // Main color: #243042
  // Gopher color: #4dabf7
  "name": "Gopher Theme",
  "colors": {
    // Editor
    "editor.background": "#243042",
    "editorCursor.foreground": "#4dabf7",
    "editor.lineHighlightBackground": "#2d3541",
    "editor.selectionBackground": "#4dabf7af",
    "editor.selectionHighlightBackground": "#1679c525",
    "editor.selectionHighlightBorder": "#4dabf781",
    "editor.wordHighlightStrongBackground": "#ffffff1d",
    "editor.wordHighlightBackground": "#238acf30",
    "editorIndentGuide.background1": "#2a3038",
    "editor.findMatchBackground": "#4dabf769", // Current search match
    "editor.findMatchHighlightBackground": "#3dd8ed37", // Other matches

    // Tabs
    "editorGroupHeader.tabsBackground": "#0000001d",
    "tab.activeBorderTop": "#4dabf7",
    "tab.inactiveBackground": "#243042",

    // Sidebar (EXPLORER panel)
    "sideBar.background": "#243042",
    "sideBar.foreground": "#e0e6f0",
    "sideBar.border": "#4dabf73d",
    "sideBarTitle.foreground": "#94a3b8",

    // File explorer items
    "list.focusBackground": "#2d3a4a",
    "list.hoverBackground": "#2a3038",
    "list.activeSelectionBackground": "#2d3a4a",
    "list.inactiveSelectionBackground": "#2a3038",
    "list.activeSelectionForeground": "#f0e5e0",
    "list.inactiveSelectionForeground": "#e0e6f0",
    "list.hoverForeground": "#e0e6f0",

    // Activity bar (top bar)
    "activityBar.background": "#243042",
    "activityBar.foreground": "#4dabf7",
    "activityBar.inactiveForeground": "#5a677d",
    "activityBarBadge.background": "#4dabf7",
    "activityBarBadge.foreground": "#1e2228",

    // Status bar
    "statusBar.background": "#243042",
    "statusBar.foreground": "#e0e6f0",
    "statusBar.border": "#000000",
    "statusBar.noFolderBackground": "#243042",
    "statusBar.debuggingBackground": "#243042",

    // Title bar
    "titleBar.activeBackground": "#243042",
    "titleBar.activeForeground": "#dceffb",
    "titleBar.inactiveBackground": "#243042",

    // Widgets
    "editorSuggestWidget.border": "#00AFAF",
    "editorHoverWidget.border": "#008888",

    // Quick input (Command Palette)
    "pickerGroup.foreground": "#FFFFFF",
    "pickerGroup.border": "#444444",
    "quickInput.background": "#243042",
    "quickInput.foreground": "#FFFFFF",
    "quickInputList.focusBackground": "#1f7dca",
    "quickInputList.focusForeground": "#FFFFFF"
  },
  "tokenColors": [
    {
      "scope": ["meta.object-literal.key.js", "meta.object-literal.key.ts", "variable.other.property.js"],
      "settings": {
        "foreground": "#eeeeee"
      }
    },
    {
      "scope": [
        "variable.other.constant.js",
        "meta.definition.variable.js",
        "meta.var-single-variable.expr.js",
        "meta.var.expr.js",
        "source.js"
      ],
      "settings": {
        "foreground": "#ffffffda"
      }
    },
    {
      "scope": ["text.html.derivative", "meta.decorator.ts", "source.ts"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": ["source.ignore"],
      "settings": {
        "foreground": "#4dabf7"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#696969"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#82e8edcb"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#eb6f92"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier", "keyword.control"],
      "settings": {
        "foreground": "#73befb"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#dfcef4"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#f1f1f1df"
      }
    },
    {
      "name": "Number, Constant, Function Argument",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#f4f0eb"
      }
    },
    {
      "name": "String, Symbols",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter"
      ],
      "settings": {
        "foreground": "#dfcef4"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#cbbaeb"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#9ccfd8"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "#99add5"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#4dabf7"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#c4a7e7"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#eb6f92"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#4dabf7"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#9ccfd8"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#9ccfd8"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#4dabf7"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#9ccfd8"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ebbcba"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold", "markup.bold string"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ebbcba"
      }
    }
  ]
}
